<h1>Listing forgotusers</h1>

<table>
  <tr>
    <th>Securityquestion</th>
    <th>Securityanswer</th>
    <th>Newpassword</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @forgotusers.each do |forgotuser| %>
  <tr>
    <td><%= forgotuser.securityQuestion %></td>
    <td><%= forgotuser.securityAnswer %></td>
    <td><%= forgotuser.newPassword %></td>
    <td><%= link_to 'Show', forgotuser %></td>
    <td><%= link_to 'Edit', edit_forgotuser_path(forgotuser) %></td>
    <td><%= link_to 'Destroy', forgotuser, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

